   ],
   "source": [
    "# 0. Imports\n",
    "# =============================\n",
    "import os, random, cv2, time, pickle\n",
    "import numpy as np\n",
    "import tensorflow as tf\n",
    "from tensorflow.keras.applications import ResNet50\n",
    "from tensorflow.keras.layers import Input, Dense, GlobalAveragePooling2D, Lambda\n",
    "from tensorflow.keras.models import Model\n",
    "from tensorflow.keras.preprocessing.image import ImageDataGenerator\n",
    "import gradio as gr\n",
    "\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.metrics import roc_curve, auc\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "# =============================\n",
    "# 1. Load CK+ images by class\n",
    "# =============================\n",
    "def load_images_by_class(data_dir, target_size=(224,224)):\n",
    "    \"\"\"\n",
    "    Load images from CK+ dataset organized in subfolders per emotion.\n",
    "    Returns a dictionary {emotion: list_of_images}.\n",
    "    \"\"\"\n",
    "    data = {}\n",
    "    for emotion in os.listdir(data_dir):\n",
    "        emotion_dir = os.path.join(data_dir, emotion)\n",
    "        if not os.path.isdir(emotion_dir):\n",
    "            continue\n",
    "        images = []\n",
    "        for img_name in os.listdir(emotion_dir):\n",
    "            img_path = os.path.join(emotion_dir, img_name)\n",
    "            img = cv2.imread(img_path)\n",
    "            if img is None:\n",
    "                continue\n",
    "            img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\n",
    "            img = cv2.resize(img, target_size)\n",
    "            img = img / 255.0\n",
    "            images.append(img)\n",
    "        if len(images) > 1: \n",
    "            data[emotion] = images\n",
    "    return data\n",
    "\n",
    "# Load CK+ dataset\n",
    "data_dir = \"/kaggle/input/ckplus/CK+48\"  \n",
    "data = load_images_by_class(data_dir)\n",
    "\n",
    "# =============================\n",
    "# 2. Generate Siamese pairs\n",
    "# =============================\n",
    "def generate_pairs(data, num_pairs=1000):\n",
    "    \"\"\"\n",
    "    Generate positive (same emotion) and negative (different emotion) pairs\n",
    "    for Siamese training.\n",
    "    \"\"\"\n",
    "    emotions = list(data.keys())\n",
    "    pairs = []\n",
    "    labels = []\n",
    "\n",
    "    for _ in range(num_pairs):\n",
    "        # Positive pair\n",
    "        emotion = random.choice(emotions)\n",
    "        img1, img2 = random.sample(data[emotion], 2)\n",
    "        pairs.append([img1, img2])\n",
    "        labels.append(1)\n",
    "\n",
    "        # Negative pair\n",
    "        emo1, emo2 = random.sample(emotions, 2)\n",
    "        img1 = random.choice(data[emo1])\n",
    "        img2 = random.choice(data[emo2])\n",
    "        pairs.append([img1, img2])\n",
    "        labels.append(0)\n",
    "\n",
    "    pairs = np.array(pairs)\n",
    "    labels = np.array(labels)\n",
    "    return [pairs[:,0], pairs[:,1]], labels\n",
    "\n",
    "(x1, x2), y = generate_pairs(data, num_pairs=1000)\n",
    "\n",
    "# =============================\n",
    "# 3. Build embedding model (ResNet50)\n",
    "# =============================\n",
    "def build_embedding_model(input_shape=(224,224,3), embedding_dim=128):\n",
    "    \"\"\"\n",
    "    ResNet50 backbone to produce emotion embeddings.\n",
    "    \"\"\"\n",
    "    base_model = ResNet50(weights=\"imagenet\", include_top=False, input_shape=input_shape)\n",
    "    base_model.trainable = True  # fine-tune\n",
    "    inputs = Input(shape=input_shape)\n",
    "    x = base_model(inputs)\n",
    "    x = GlobalAveragePooling2D()(x)\n",
    "    x = Dense(embedding_dim, activation=\"relu\")(x)\n",
    "    x = Lambda(lambda x: tf.math.l2_normalize(x, axis=1))(x)\n",
    "    return Model(inputs, x, name=\"ResNet50_Embedding\")\n",
    "\n",
    "embedding_model = build_embedding_model()\n",
    "\n",
    "# =============================\n",
    "# 4. Build Siamese network\n",
    "# =============================\n",
    "def euclidean_distance(vectors):\n",
    "    x, y = vectors\n",
    "    return tf.sqrt(tf.maximum(tf.reduce_sum(tf.square(x - y), axis=1, keepdims=True), tf.keras.backend.epsilon()))\n",
    "\n",
    "def build_siamese_network(embedding_model, input_shape=(224,224,3)):\n",
    "    input_a = Input(shape=input_shape)\n",
    "    input_b = Input(shape=input_shape)\n",
    "    embed_a = embedding_model(input_a)\n",
    "    embed_b = embedding_model(input_b)\n",
    "    distance = Lambda(euclidean_distance)([embed_a, embed_b])\n",
    "    return Model([input_a, input_b], distance, name=\"Siamese_ResNet50\")\n",
    "\n",
    "siamese_model = build_siamese_network(embedding_model)\n",
    "\n",
    "# =============================\n",
    "# 5. Contrastive loss\n",
    "# =============================\n",
    "\n",
    "def contrastive_loss(y_true, y_pred, margin=1.0):\n",
    "    y_true = tf.cast(y_true, y_pred.dtype)\n",
    "    squared_pred = tf.square(y_pred)\n",
    "    squared_margin = tf.square(tf.maximum(margin - y_pred, 0))\n",
    "    return tf.reduce_mean(y_true * squared_pred + (1 - y_true) * squared_margin)\n",
    "    \n",
    "\n",
    "siamese_model.compile(optimizer=tf.keras.optimizers.Adam(1e-4), loss=contrastive_loss)\n",
    "# =============================\n",
    "# 5b. Siamese Accuracy Metric\n",
    "# =============================\n",
    "\n",
    "\n",
    "# =============================\n",
    "# 6. Training Siamese network\n",
    "# =============================\n",
    "datagen = ImageDataGenerator(\n",
    "    rotation_range=10,\n",
    "    zoom_range=0.1,\n",
    "    width_shift_range=0.1,\n",
    "    height_shift_range=0.1,\n",
    "    horizontal_flip=True\n",
    ")\n",
    "\n",
    "# Train\n",
    "x1 = np.array(x1)\n",
    "x2 = np.array(x2)\n",
    "y = np.array(y)\n",
    "\n",
    "x1_train, x1_val, x2_train, x2_val, y_train, y_val = train_test_split(\n",
    "    x1, x2, y, test_size=0.2, random_state=42, stratify=y\n",
    ")\n",
    "\n",
    "history = siamese_model.fit(\n",
    "    [x1_train, x2_train],\n",
    "    y_train,\n",
    "    batch_size=16,\n",
    "    epochs=20,\n",
    "    validation_data=([x1_val, x2_val], y_val)\n",
    ")\n",
    "\n",
    "val_pred = siamese_model.predict([x1_val, x2_val])\n",
    "fpr, tpr, thresholds = roc_curve(y_val, val_pred)\n",
    "best_idx = np.argmax(tpr - fpr)\n",
    "best_threshold = thresholds[best_idx]\n",
    "print(\"Best threshold:\", best_threshold)\n",
    "\n",
    "# =============================\n",
    "# 7. Compute emotion prototypes for inference\n",
    "# =============================\n",
    "def compute_prototypes(embedding_model, data):\n",
    "    \"\"\"\n",
    "    Compute mean embedding per emotion class for inference.\n",
    "    \"\"\"\n",
    "    prototypes = {}\n",
    "    for emotion, images in data.items():\n",
    "        embeddings = embedding_model.predict(np.array(images), batch_size=16)\n",
    "        prototypes[emotion] = np.mean(embeddings, axis=0)\n",
    "    return prototypes\n",
    "\n",
    "prototypes = compute_prototypes(embedding_model, data)\n",
    "\n",
    "# Save prototypes\n",
    "with open(\"prototypes.pkl\", \"wb\") as f:\n",
    "    pickle.dump(prototypes, f)\n",
    "\n",
    "# =============================\n",
    "# 8. Prediction function\n",
    "# =============================\n",
    "def predict_emotion(face_image):\n",
    "    \"\"\"\n",
    "    Predict emotion based on nearest prototype in embedding space.\n",
    "    \"\"\"\n",
    "    img = cv2.resize(face_image, (224,224))\n",
    "    img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB) / 255.0\n",
    "    img = np.expand_dims(img, axis=0)\n",
    "\n",
    "    embedding = embedding_model.predict(img)[0]\n",
    "\n",
    "    min_dist = float('inf')\n",
    "    pred_emotion = None\n",
    "    for emo, proto in prototypes.items():\n",
    "        dist = np.linalg.norm(embedding - proto)\n",
    "        if dist < min_dist:\n",
    "            min_dist = dist\n",
    "            pred_emotion = emo\n",
    "\n",
    "    confidence = 1 / (1 + min_dist)\n",
    "    return pred_emotion, confidence\n",
    "\n",
    "# =============================\n",
    "# 9. Gradio Interface\n",
    "# =============================\n",
    "def emotion_recognition(image):\n",
    "    \"\"\"\n",
    "    Gradio function: input image -> predicted emotion + confidence + time.\n",
    "    \"\"\"\n",
    "    if image is None:\n",
    "        return \"No image provided\", None, None\n",
    "    start_time = time.time()\n",
    "    face = image\n",
    "    emotion, confidence = predict_emotion(face)\n",
    "    elapsed_time = time.time() - start_time\n",
    "    return emotion, float(confidence), round(elapsed_time, 3) "
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 3,
   "id": "6fa3c811",
