    "# =============================\n",
    "# 10. Gradio interface (fixed)\n",
    "# =============================\n",
    "def gradio_predict(img):\n",
    "    \"\"\"\n",
    "    Input: np.array from Gradio\n",
    "    Returns: predicted emotion, confidence, similarity dict\n",
    "    \"\"\"\n",
    "    pred, conf, sims = predict_emotion(img)\n",
    "    # ensure sims is a dict for gr.JSON\n",
    "    return pred, float(conf), sims\n",
    "\n",
    "import gradio as gr\n",
    "\n",
    "iface = gr.Interface(\n",
    "    fn=gradio_predict,\n",
    "    inputs=gr.Image(type=\"numpy\"),\n",
    "    outputs=[\n",
    "        gr.Textbox(label=\"Predicted Emotion\"),\n",
    "        gr.Number(label=\"Confidence\"),\n",
    "        gr.JSON(label=\"Similarity Scores\")  # expects dict, not string\n",
    "    ],\n",
    "    title=\"CK+ Emotion Recognition SNN\",\n",
    "    description=\"Siamese Network trained on CK+ with contrastive loss.\"\n",
    ")\n",
    "\n",
    "# Launch interface\n",
    "iface.launch()\n"
   ]
