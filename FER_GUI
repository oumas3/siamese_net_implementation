      "\u001b[1m17/17\u001b[0m \u001b[32m━━━━━━━━━━━━━━━━━━━━\u001b[0m\u001b[37m\u001b[0m \u001b[1m1s\u001b[0m 31ms/step\n"
     ]
    }
   ],
   "source": [
    "# =============================\n",
    "# 0. Imports\n",
    "# =============================\n",
    "import os, random, cv2, time, pickle\n",
    "import numpy as np\n",
    "import tensorflow as tf\n",
    "from tensorflow.keras.applications import ResNet50\n",
    "from tensorflow.keras.layers import Input, Dense, GlobalAveragePooling2D, Lambda, Dropout\n",
    "from tensorflow.keras.models import Model\n",
    "from tensorflow.keras.preprocessing.image import ImageDataGenerator\n",
    "from tensorflow.keras.callbacks import EarlyStopping\n",
    "from sklearn.model_selection import train_test_split\n",
    "from sklearn.metrics import accuracy_score, confusion_matrix, roc_auc_score\n",
    "import matplotlib.pyplot as plt\n",
    "import gradio as gr\n",
    "\n",
    "# =============================\n",
    "# 1. Load CK+ dataset\n",
    "# =============================\n",
    "def load_images_by_class(data_dir, target_size=(224,224)):\n",
    "    \"\"\"\n",
    "    Load images from CK+ dataset organized in subfolders per emotion.\n",
    "    Returns: dict {emotion: list of images (np.array)}\n",
    "    \"\"\"\n",
    "    data = {}\n",
    "    for emotion in os.listdir(data_dir):\n",
    "        emotion_dir = os.path.join(data_dir, emotion)\n",
    "        if not os.path.isdir(emotion_dir):\n",
    "            continue\n",
    "        images = []\n",
    "        for img_name in os.listdir(emotion_dir):\n",
    "            img_path = os.path.join(emotion_dir, img_name)\n",
    "            img = cv2.imread(img_path)\n",
    "            if img is None:\n",
    "                continue\n",
    "            img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\n",
    "            img = cv2.resize(img, target_size)\n",
    "            img = img / 255.0\n",
    "            images.append(img)\n",
    "        if len(images) > 1:  # need at least 2 images for positive pair\n",
    "            data[emotion] = images\n",
    "    return data\n",
    "\n",
    "data_dir = \"/kaggle/input/ckplus/CK+48\"  # change path\n",
    "data = load_images_by_class(data_dir)\n",
    "class_names = list(data.keys())\n",
    "NUM_CLASSES = len(class_names)\n",
    "\n",
    "# =============================\n",
    "# 2. Generate Siamese pairs\n",
    "# =============================\n",
    "def generate_pairs(data, num_pairs_per_class=50):\n",
    "    \"\"\"\n",
    "    Generate positive and negative pairs for Siamese training.\n",
    "    \"\"\"\n",
    "    pairs = []\n",
    "    labels = []\n",
    "    emotions = list(data.keys())\n",
    "\n",
    "    for emotion in emotions:\n",
    "        imgs = data[emotion]\n",
    "        for _ in range(num_pairs_per_class):\n",
    "            # Positive pair\n",
    "            img1, img2 = random.sample(imgs, 2)\n",
    "            pairs.append([img1, img2])\n",
    "            labels.append(1)\n",
    "            # Negative pair\n",
    "            neg_emotion = random.choice([e for e in emotions if e != emotion])\n",
    "            img1 = random.choice(imgs)\n",
    "            img2 = random.choice(data[neg_emotion])\n",
    "            pairs.append([img1, img2])\n",
    "            labels.append(0)\n",
    "\n",
    "    pairs = np.array(pairs)\n",
    "    labels = np.array(labels)\n",
    "    return [pairs[:,0], pairs[:,1]], labels\n",
    "    \n",
    "\n",
    "(x1, x2), y = generate_pairs(data, num_pairs_per_class=50)\n",
    "\n",
    "\n",
    "# =============================\n",
    "# 3. Build embedding model\n",
    "# =============================\n",
    "def build_embedding_model(input_shape=(224,224,3), embedding_dim=128):\n",
    "    base_model = ResNet50(weights='imagenet', include_top=False, input_shape=input_shape)\n",
    "    base_model.trainable = True  # fine-tune for CK+ emotions\n",
    "    inp = Input(shape=input_shape)\n",
    "    x = base_model(inp)\n",
    "    x = GlobalAveragePooling2D()(x)\n",
    "    x = Dense(embedding_dim, activation='relu')(x)\n",
    "    x = Dropout(0.3)(x)\n",
    "    x = Lambda(lambda x: tf.math.l2_normalize(x, axis=1))(x)  # normalize embeddings\n",
    "    return Model(inp, x, name=\"Embedding_Model\")\n",
    "\n",
    "embedding_model = build_embedding_model()\n",
    "\n",
    "# =============================\n",
    "# 4. Build Siamese network (Cosine distance)\n",
    "# =============================\n",
    "def cosine_distance(vectors):\n",
    "    x, y = vectors\n",
    "    return 1 - tf.reduce_sum(x * y, axis=1, keepdims=True)\n",
    "\n",
    "input_a = Input(shape=(224,224,3))\n",
    "input_b = Input(shape=(224,224,3))\n",
    "embed_a = embedding_model(input_a)\n",
    "embed_b = embedding_model(input_b)\n",
    "distance = Lambda(cosine_distance)([embed_a, embed_b])\n",
    "siamese_model = Model([input_a, input_b], distance, name=\"Siamese_Model\")\n",
    "\n",
    "# =============================\n",
    "# 5. Contrastive loss\n",
    "# =============================\n",
    "def contrastive_loss(y_true, y_pred, margin=0.5):\n",
    "    y_true = tf.cast(y_true, y_pred.dtype)\n",
    "    squared_pred = tf.square(y_pred)\n",
    "    squared_margin = tf.square(tf.maximum(margin - y_pred, 0))\n",
    "    return tf.reduce_mean(y_true * squared_pred + (1 - y_true) * squared_margin)\n",
    "\n",
    "siamese_model.compile(optimizer=tf.keras.optimizers.Adam(1e-4), loss=contrastive_loss)\n",
    "\n",
    "# =============================\n",
    "# 6. Train Siamese network\n",
    "# =============================\n",
    "x1 = np.array(x1)\n",
    "x2 = np.array(x2)\n",
    "y = np.array(y)\n",
    "\n",
    "x1_train, x1_val, x2_train, x2_val, y_train, y_val = train_test_split(\n",
    "    x1, x2, y, test_size=0.2, stratify=y, random_state=42\n",
    ")\n",
    "\n",
    "datagen = ImageDataGenerator(\n",
    "    rotation_range=15,\n",
    "    zoom_range=0.1,\n",
    "    width_shift_range=0.1,\n",
    "    height_shift_range=0.1,\n",
    "    horizontal_flip=True\n",
    ")\n",
    "\n",
    "early_stop = EarlyStopping(monitor='val_loss', patience=5, restore_best_weights=True)\n",
    "\n",
    "history = siamese_model.fit(\n",
    "    [x1_train, x2_train], y_train,\n",
    "    batch_size=8,  # reduce if GPU crashes\n",
    "    epochs=30,\n",
    "    validation_data=([x1_val, x2_val], y_val),\n",
    "    callbacks=[early_stop]\n",
    ")\n",
    "\n",
    "# =============================\n",
    "# 7. Compute emotion prototypes\n",
    "# =============================\n",
    "def compute_prototypes(embedding_model, data):\n",
    "    prototypes = {}\n",
    "    for emotion, imgs in data.items():\n",
    "        imgs = np.array(imgs)\n",
    "        emb = embedding_model.predict(imgs, batch_size=8)\n",
    "        prototypes[emotion] = np.mean(emb, axis=0)\n",
    "    return prototypes\n",
    "\n",
    "prototypes = compute_prototypes(embedding_model, data)\n",
    "with open(\"prototypes.pkl\", \"wb\") as f:\n",
    "    pickle.dump(prototypes, f)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 20,
   "id": "cd829ce6",
   "metadata": {
