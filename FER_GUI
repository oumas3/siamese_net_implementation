   "source": [
    "def predict_emotion(img):\n",
    "    img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\n",
    "    img = cv2.resize(img, (224,224)) / 255.0\n",
    "    emb = embedding_model.predict(np.expand_dims(img,0), verbose=0)[0]\n",
    "\n",
    "    sims = {}\n",
    "    for emo in class_names:\n",
    "        sims[emo] = float(np.dot(emb, prototypes[emo]))  # cosine similarity\n",
    "\n",
    "    pred_emo = max(sims, key=sims.get)\n",
    "    confidence = sims[pred_emo]\n",
    "    return pred_emo, confidence, sims\n"
   ]
  },
  {
