   ],
   "source": [
    "# =============================\n",
    "# 10. Evaluation Metrics for SNN\n",
    "# =============================\n",
    "from sklearn.metrics import accuracy_score, roc_auc_score, roc_curve\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def evaluate_snn(x1_val, x2_val, y_val, siamese_model, threshold=None):\n",
    "    \"\"\"\n",
    "    Evaluate Siamese Network embeddings:\n",
    "    - Cosine similarity for positive/negative pairs\n",
    "    - ROC-AUC\n",
    "    - Accuracy at a threshold\n",
    "    - Equal Error Rate (EER)\n",
    "    \"\"\"\n",
    "\n",
    "    # 1. Predict distances (cosine distance)\n",
    "    distances = siamese_model.predict([x1_val, x2_val], batch_size=16)\n",
    "    distances = distances.flatten()  # shape = (num_samples,)\n",
    "\n",
    "    # 2. ROC Curve & AUC\n",
    "    fpr, tpr, thresholds = roc_curve(y_val, 1 - distances)  # 1-distance = similarity\n",
    "    roc_auc = auc(fpr, tpr)\n",
    "    \n",
    "    # 3. Equal Error Rate (EER)\n",
    "    fnr = 1 - tpr\n",
    "    eer_idx = np.nanargmin(np.absolute(fnr - fpr))\n",
    "    eer = (fpr[eer_idx] + fnr[eer_idx]) / 2\n",
    "    eer_threshold = thresholds[eer_idx]\n",
    "\n",
    "    # 4. Accuracy at threshold\n",
    "    if threshold is None:\n",
    "        threshold = eer_threshold  # use EER threshold if not given\n",
    "    y_pred = (distances < threshold).astype(int)  # closer = same emotion\n",
    "    acc = accuracy_score(y_val, y_pred)\n",
    "\n",
    "    # 5. Positive vs Negative cosine similarity statistics\n",
    "    pos_sims = 1 - distances[y_val == 1]\n",
    "    neg_sims = 1 - distances[y_val == 0]\n",
    "    mean_pos = np.mean(pos_sims)\n",
    "    mean_neg = np.mean(neg_sims)\n",
    "    std_pos = np.std(pos_sims)\n",
    "    std_neg = np.std(neg_sims)\n",
    "\n",
    "    print(f\"ROC-AUC: {roc_auc:.4f}\")\n",
    "    print(f\"EER: {eer:.4f} at threshold {eer_threshold:.4f}\")\n",
    "    print(f\"Accuracy @ threshold {threshold:.4f}: {acc:.4f}\")\n",
    "    print(f\"Positive pairs similarity: mean={mean_pos:.4f}, std={std_pos:.4f}\")\n",
    "    print(f\"Negative pairs similarity: mean={mean_neg:.4f}, std={std_neg:.4f}\")\n",
    "\n",
    "    # 6. Plot ROC curve\n",
    "    plt.figure(figsize=(6,6))\n",
    "    plt.plot(fpr, tpr, label=f'ROC Curve (AUC = {roc_auc:.4f})')\n",
    "    plt.plot([0,1], [0,1], 'k--')\n",
    "    plt.xlabel(\"False Positive Rate\")\n",
    "    plt.ylabel(\"True Positive Rate\")\n",
    "    plt.title(\"ROC Curve for Siamese Network\")\n",
    "    plt.legend()\n",
    "    plt.show()\n",
    "\n",
    "    return distances, y_pred, threshold\n",
    "\n",
    "# Usage example:\n",
    "distances, y_pred, threshold = evaluate_snn(x1_val, x2_val, y_val, siamese_model)"
   ]
  },
