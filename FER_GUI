   "source": [
    "import gradio as gr\n",
    "\n",
    "def gradio_predict(img):\n",
    "    pred, conf, sims = predict_emotion(img)\n",
    "    sims_sorted = sorted(sims.items(), key=lambda x: x[1], reverse=True)\n",
    "    table_text = \"\\n\".join([f\"{emo}: {sim:.4f}\" for emo, sim in sims_sorted])\n",
    "    return pred, conf, table_text\n",
    "\n",
    "iface = gr.Interface(\n",
    "    fn=gradio_predict,\n",
    "    inputs=gr.Image(type=\"numpy\"),\n",
    "    outputs=[gr.Text(label=\"Predicted Emotion\"),\n",
    "             gr.Text(label=\"Confidence\"),\n",
    "             gr.Text(label=\"Similarity Scores\")],\n",
    "    title=\"CK+ Siamese Emotion Recognition\",\n",
    "    description=\"Upload a face image to see predicted emotion, confidence, and similarity to prototypes\"\n",
    ")\n",
    "\n",
    "iface.launch()"
   ]
