    "def cosine_distance(vectors):\n",
    "    x, y = vectors\n",
    "    return 1 - tf.reduce_sum(x * y, axis=1, keepdims=True)  # cosine similarity distance\n",
    "\n",
    "def build_siamese_network(embedding_model, input_shape=(224,224,3)):\n",
    "    inp_a = Input(shape=input_shape)\n",
    "    inp_b = Input(shape=input_shape)\n",
    "    emb_a = embedding_model(inp_a)\n",
    "    emb_b = embedding_model(inp_b)\n",
    "    dist = Lambda(cosine_distance)([emb_a, emb_b])\n",
    "    return Model([inp_a, inp_b], dist, name=\"Siamese_Network\")\n",
    "\n",
    "siamese_model = build_siamese_network(embedding_model)\n"
   ]
  },
