   "source": [
    "prototypes = {}\n",
    "for idx, emo in enumerate(class_names):\n",
    "    imgs = np.array(data[emo])\n",
    "    emb = embedding_model.predict(imgs, batch_size=4, verbose=0)\n",
    "    prototypes[emo] = np.mean(emb, axis=0)  # prototype = mean embedding\n",
    "\n",
    "# Save prototypes for later\n",
    "with open(\"prototypes.pkl\", \"wb\") as f:\n",
    "    pickle.dump(prototypes, f)"
   ]
