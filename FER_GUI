   "outputs": [],
   "source": [
    "\n",
    "# =============================\n",
    "# 8. Prediction function (fixed)\n",
    "# =============================\n",
    "def predict_emotion(img):\n",
    "    \"\"\"\n",
    "    Input: np.array image (from OpenCV or Gradio)\n",
    "    Returns: predicted emotion, confidence, similarity scores dict\n",
    "    \"\"\"\n",
    "    # Convert to float32 if not already\n",
    "    if img.dtype != np.float32:\n",
    "        img = img.astype(np.float32)\n",
    "\n",
    "    # If values are [0,255], normalize\n",
    "    if img.max() > 1.0:\n",
    "        img = img / 255.0\n",
    "\n",
    "    # OpenCV needs float32 for cvtColor\n",
    "    if img.shape[2] == 3:\n",
    "        img = cv2.cvtColor(img, cv2.COLOR_BGR2RGB)\n",
    "\n",
    "    # Resize to model input\n",
    "    img = cv2.resize(img, (224,224))\n",
    "\n",
    "    # Add batch dimension\n",
    "    emb = embedding_model.predict(np.expand_dims(img,0), verbose=0)[0]\n",
    "\n",
    "    # Compute similarity with prototypes\n",
    "    sims = {emo: float(np.dot(emb, proto)) for emo, proto in prototypes.items()}\n",
    "\n",
    "    pred_emotion = max(sims, key=sims.get)\n",
    "    confidence = sims[pred_emotion]\n",
    "\n",
    "    return pred_emotion, confidence, sims\n",
    "\n",
    "\n"
   ]
  },
