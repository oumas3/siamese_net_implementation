    "def plot_new_face_embedding(face_image):\n",
    "    # Compute new embedding\n",
    "    emb_new = embedding_model.predict(np.expand_dims(cv2.resize(cv2.cvtColor(face_image, cv2.COLOR_BGR2RGB)/255.0, (224,224)),0))[0]\n",
    "    \n",
    "    # Combine with dataset embeddings\n",
    "    emb_combined = np.vstack([all_embeddings, emb_new])\n",
    "    \n",
    "    # t-SNE again (small example, or reuse previous)\n",
    "    tsne_combined = TSNE(n_components=2, random_state=42, perplexity=30)\n",
    "    emb_2d_combined = tsne_combined.fit_transform(emb_combined)\n",
    "    \n",
    "    emb_2d_existing = emb_2d_combined[:-1]\n",
    "    emb_2d_new = emb_2d_combined[-1]\n",
    "    \n",
    "    # Plot\n",
    "    plt.figure(figsize=(8,6))\n",
    "    for emo in np.unique(all_labels):\n",
    "        idx = np.where(all_labels == emo)\n",
    "        plt.scatter(emb_2d_existing[idx,0], emb_2d_existing[idx,1], label=emo, alpha=0.6)\n",
    "    \n",
    "    # Plot new face\n",
    "    plt.scatter(emb_2d_new[0], emb_2d_new[1], color='black', marker='X', s=150, label='New Face')\n",
    "    plt.legend()\n",
    "    plt.title(\"New face embedding relative to CK+ clusters\")\n",
    "    plt.show()\n",
    "    \n",
    "    # Also show predicted emotion\n",
    "    pred_emotion, _ = predict_emotion(face_image)\n",
    "    print(\"Predicted emotion:\", pred_emotion)\n"
   ]
