    "def make_tf_dataset(data, batch_size=4, target_size=(224,224)):\n",
    "    \"\"\"\n",
    "    Creates a tf.data.Dataset yielding ([x1, x2], y) tensors.\n",
    "    \"\"\"\n",
    "    emotions = list(data.keys())\n",
    "\n",
    "    def gen():\n",
    "        while True:\n",
    "            x1_batch = np.zeros((batch_size*2, *target_size, 3), dtype=np.float32)\n",
    "            x2_batch = np.zeros((batch_size*2, *target_size, 3), dtype=np.float32)\n",
    "            y_batch = np.zeros((batch_size*2,), dtype=np.float32)\n",
    "\n",
    "            for i in range(batch_size):\n",
    "                # Positive pair\n",
    "                emo = random.choice(emotions)\n",
    "                img1, img2 = random.sample(data[emo], 2)\n",
    "                x1_batch[i] = cv2.resize(img1, target_size) / 255.0\n",
    "                x2_batch[i] = cv2.resize(img2, target_size) / 255.0\n",
    "                y_batch[i] = 1.0\n",
    "\n",
    "                # Negative pair\n",
    "                emo1, emo2 = random.sample(emotions, 2)\n",
    "                x1_batch[batch_size+i] = cv2.resize(random.choice(data[emo1]), target_size) / 255.0\n",
    "                x2_batch[batch_size+i] = cv2.resize(random.choice(data[emo2]), target_size) / 255.0\n",
    "                y_batch[batch_size+i] = 0.0\n",
    "\n",
    "            yield (x1_batch, x2_batch), y_batch\n",
    "\n",
    "    output_signature = (\n",
    "        (\n",
    "            tf.TensorSpec(shape=(batch_size*2, *target_size, 3), dtype=tf.float32),\n",
    "            tf.TensorSpec(shape=(batch_size*2, *target_size, 3), dtype=tf.float32),\n",
    "        ),\n",
    "        tf.TensorSpec(shape=(batch_size*2,), dtype=tf.float32)\n",
    "    )\n",
    "\n",
    "    ds = tf.data.Dataset.from_generator(gen, output_signature=output_signature)\n",
    "    ds = ds.prefetch(tf.data.AUTOTUNE)\n",
    "    return ds\n",
    "\n",
    "# Create datasets\n",
    "batch_size = 4\n",
    "train_ds = make_tf_dataset(data, batch_size=batch_size)\n",
    "val_ds = make_tf_dataset(data, batch_size=batch_size)\n",
    "\n",
    "# Train safely\n",
    "steps_per_epoch = 50\n",
    "val_steps = 10\n",
    "epochs = 20\n",
    "\n",
    "early_stop = EarlyStopping(monitor='val_loss', patience=5, restore_best_weights=True)\n",
    "\n",
    "history = siamese_model.fit(\n",
    "    train_ds,\n",
    "    steps_per_epoch=steps_per_epoch,\n",
    "    validation_data=val_ds,\n",
    "    validation_steps=val_steps,\n",
    "    epochs=epochs,\n",
    "    callbacks=[early_stop]\n",
    ")\n"
   ]
  },
