    "def siamese_pair_generator(data, batch_size=4, target_size=(224,224)):\n",
    "    emotions = list(data.keys())\n",
    "    while True:\n",
    "        x1_batch = np.zeros((batch_size*2, *target_size, 3), dtype=np.float32)\n",
    "        x2_batch = np.zeros((batch_size*2, *target_size, 3), dtype=np.float32)\n",
    "        y_batch = np.zeros((batch_size*2,), dtype=np.float32)\n",
    "\n",
    "        for i in range(batch_size):\n",
    "            # Positive pair\n",
    "            emo = random.choice(emotions)\n",
    "            img1, img2 = random.sample(data[emo], 2)\n",
    "            x1_batch[i] = cv2.resize(img1, target_size) / 255.0\n",
    "            x2_batch[i] = cv2.resize(img2, target_size) / 255.0\n",
    "            y_batch[i] = 1.0\n",
    "\n",
    "            # Negative pair\n",
    "            emo1, emo2 = random.sample(emotions, 2)\n",
    "            x1_batch[batch_size+i] = cv2.resize(random.choice(data[emo1]), target_size) / 255.0\n",
    "            x2_batch[batch_size+i] = cv2.resize(random.choice(data[emo2]), target_size) / 255.0\n",
    "            y_batch[batch_size+i] = 0.0\n",
    "\n",
    "        yield [x1_batch, x2_batch], y_batch\n"
   ]
  },
