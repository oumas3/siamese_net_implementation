    "def build_embedding_model(input_shape=(224,224,3), embedding_dim=128):\n",
    "    base = ResNet50(weights=\"imagenet\", include_top=False, input_shape=input_shape)\n",
    "    base.trainable = False  # freeze backbone first for stability\n",
    "\n",
    "    inp = Input(shape=input_shape)\n",
    "    x = base(inp, training=False)\n",
    "    x = GlobalAveragePooling2D()(x)\n",
    "    x = Dense(embedding_dim, activation=\"relu\")(x)\n",
    "    x = Dropout(0.3)(x)\n",
    "    x = Lambda(lambda x: tf.math.l2_normalize(x, axis=1))(x)  # normalize for cosine\n",
    "    return Model(inp, x, name=\"Embedding_Model\")\n",
    "\n",
    "embedding_model = build_embedding_model()\n",
    "embedding_model.summary()\n"
   ]
  },
